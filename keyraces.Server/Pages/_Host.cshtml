@page "/"
@namespace keyraces.Server.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = "_Layout";
}

<component type="typeof(App)" render-mode="ServerPrerendered" />

<script>
    window.authInterop = {
      fetchWithCredentials: (url, opts) =>
        fetch(url, opts).then(r => r.status)
    };
</script>